<h1>投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>地名 / 駅名</h4>
  <%= f.text_field :address, placeholder: "住所" %>
  <%#= f.text_field :place %>
  <!--<h4>駅名</h4>-->
  <!--<%#= f.text_field :station %>-->
  <h4>投稿内容</h4>
  <%= f.text_area :writings %>
  <%= f.submit '投稿' %>
<% end %>