<header>
  <nav class="navbar navbar-expand-lg navbar-light">
    <!--<div class="container">-->
      <a class="logo" href="/"><%= image_tag　asset_path('logo.jpg') %></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavDropdown">
        <ul class="navbar-nav ml-auto ">
        <!--<%# if user_signed_in? %> → devise側で用意しているヘルパー-->
        <% if user_signed_in? %>
          <li><%= link_to "Home", root_path, class: "btn btn-outline-dark" %></li>
          <li><%= link_to "マイページ", user_path(current_user), class: "btn btn-outline-dark" %></li>
          <li><%= link_to "投稿一覧", posts_path, class: "btn btn-outline-dark" %></li>
          <!--method: :deleteでdestroy_user_session_pathへルーティングさせる-->
          <li><%= link_to "ログアウト", destroy_user_session_path, method: :delete,class: "btn btn-outline-dark" %></li>
        <% else %>
          <li><%= link_to "Home", root_path, class: "btn btn-outline-dark" %></li>
          <li><%= link_to "投稿一覧", posts_path, class: "btn btn-outline-dark" %></li>
          <li><%= link_to "新規登録", new_user_registration_path, class: "btn btn-outline-dark" %></li>
          <li><%= link_to "ログイン", new_user_session_path, class: "btn btn-outline-dark" %></li>
        <% end %>
        </ul>
      </div>
    <!--</div>-->
  </nav>
</header>