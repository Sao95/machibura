<div>
<h3><%= @user.name %></h3>
</div>

<% @posts.each do |post| %>
<div>
  <%= attachment_image_tag post, :image %>
  <p> 投稿文</p>
  <div><%= post.writings %></div>
  <%= link_to "詳細",  post_path(post.id) %>
</div>
<% end %>

<% if @user.id == current_user.id %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <%= form_with model:@user, local:true do |f| %>
    <p><%= link_to "退会する", unsubscribe_path(@user), class: "btn btn-danger" %><p>
  <% end %>
<% end %>